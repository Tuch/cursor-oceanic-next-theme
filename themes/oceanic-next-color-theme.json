  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {"foreground": "#65737E"}
    },
    {
      "scope": ["string", "string.quoted"],
      "settings": {"foreground": "#99C794"}
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {"foreground": "#C594C5"}
    },
    {
      "scope": ["entity.name.type", "support.type"],
      "settings": {"foreground": "#5FB3B3"}
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {"foreground": "#6699CC"}
    },
    {
      "scope": ["variable", "variable.other"],
      "settings": {"foreground": "#CDD3DE"}
    },
    {
      "scope": ["constant.numeric"],
      "settings": {"foreground": "#F99157"}
    },
    {
      "scope": ["keyword.operator"],
      "settings": {"foreground": "#EC5F67"}
    },
    {
      "scope": ["entity.name.tag"],
      "settings": {"foreground": "#EC5F67"}
    },
    {
      "scope": ["constant.language"],
      "settings": {"foreground": "#C594C5"}
    }
  ]
